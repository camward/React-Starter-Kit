  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack serve",
    "start": "cross-env REACT_APP_PUBLIC_URL=/ REACT_APP_SITE_URL=/ REACT_APP_API_URL=/api/v1 npm run serve",
    "start:dev": "cross-env REACT_APP_USE_MOCKS=true npm run start",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:dev": "cross-env REACT_APP_PUBLIC_URL=/ REACT_APP_SITE_URL=/ REACT_APP_API_URL=/api/v1 npm run build",
    "build:analyser": "cross-env BUNDLE_ANALYSER=true npm run build",
    "test": "jest --coverage --watchAll",
    "reinstall": "rm -rf ./node_modules && npm install",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "lint-css": "stylelint src/**/*.scss",
    "lint": "eslint --debug src/**/*.{ts,tsx}",
    "lint:write": "eslint --debug src/**/*.{ts,tsx} --fix"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
